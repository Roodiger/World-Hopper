config_version=4

_global_script_classes=[ {
"base": "",
"class": "GitAPI",
"language": "NativeScript",
"path": "res://git_api.gdns"
}, {
"base": "Node",
"class": "SoundManagerModule",
"language": "GDScript",
"path": "res://addons/sound_manager/module/internal_scripts/SoundManager_config.gd"
} ]
_global_script_class_icons={
"GitAPI": "",
"SoundManagerModule": ""
}

[application]

config/name="World Hopper"
run/main_scene="res://Start.tscn"
boot_splash/image="res://Splash.png"
boot_splash/use_filter=false
config/icon="res://planetPic.png"
config/macos_native_icon="res://planetPic.icns"
config/windows_native_icon="res://planetPic.ico"

[autoload]

Global="*res://Global.tscn"
SilentWolf="*res://addons/silent_wolf/SilentWolf.gd"
SoundManager="*res://addons/sound_manager/module/SoundManager.tscn"

[display]

window/size/width=1920
window/size/height=1080
window/size/fullscreen=true
window/handheld/orientation="sensor_landscape"
window/stretch/mode="2d"

[editor_plugins]

enabled=PoolStringArray( "silent_wolf", "sound_manager" )

[gdnative]

singletons=[ "res://git_api.gdnlib" ]

